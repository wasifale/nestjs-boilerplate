{
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": ["@typescript-eslint"],
    "env": {
      "es2020": true,
      "node": true
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "error"
    },
    "settings": {
    },
    "parserOptions": {
      "ecmaFeatures": {
          "modules": true,
          "legacyDecorators": true
      }
    },
    "parser": "@typescript-eslint/parser"
  }